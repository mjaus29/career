generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Progress {
  id        String   @id @default(cuid())
  name      String   @unique
  value     Float
  target    Float?
  unit      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}